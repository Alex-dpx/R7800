Package: qca-hostap
Version: 10.4-4.0.1756.093-1
Depends: +libc +USE_EGLIBC:librt +USE_EGLIBC:libpthread kmod-qca-wifi @(!(PACKAGE_hostapd)) +libnl-bf +qsdk-wifi-script
Menu-Depends: 
Provides: 
Build-Depends: qca-wifi
Section: QCA
Category: QCA Proprietary software
Title: Hostap authenticator
Maintainer: Qualcomm Atheros
Source: 
Type: ipkg
Description:  This package contains the IEEE 802.1x/WPA/EAP/RADIUS Authenticator.
http://www.qca.qualcomm.com
Qualcomm Atheros
@@
Config:
	menu "custom source directory"
		depends on PACKAGE_qca-hostap
	config qca-hostap_USE_CUSTOM_SOURCE_DIR
		depends on PACKAGE_qca-hostap
		bool "Use custom source directory"
		default n
	config qca-hostap_CUSTOM_SOURCE_DIR
		depends on qca-hostap_USE_CUSTOM_SOURCE_DIR
		string "Custom source directory"
		default ""
	endmenu
	menu "overwrite package version"
		depends on PACKAGE_qca-hostap
	config qca-hostap_USE_CUSTOM_VERSION
		depends on PACKAGE_qca-hostap
		bool "Use custom package version"
		default n

	config qca-hostap_CUSTOM_VERSION
		depends on qca-hostap_USE_CUSTOM_VERSION
		string "qca-hostap version as string (default version: 10.4-4.0.1756.093)"
		default "10.4-4.0.1756.093"

	endmenu
	choice
		prompt "hostap configuration"
		depends PACKAGE_qca-hostap
	
	endchoice

	config QCA_ATHR_HOSTAP_HOSTAPD_CONFIG
		string
	


  source "package/qca-hostap/Config.in.hostap"
@@

